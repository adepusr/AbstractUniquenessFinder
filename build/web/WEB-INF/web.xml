<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>FacregActionServlet</servlet-name>
        <servlet-class>FacregActionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>facultyloginActionServlet</servlet-name>
        <servlet-class>facultyloginActionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentregActionServlet</servlet-name>
        <servlet-class>StudentregActionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentloginActionServlet</servlet-name>
        <servlet-class>StudentloginActionServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FacregActionServlet</servlet-name>
        <url-pattern>/FacregActionServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>facultyloginActionServlet</servlet-name>
        <url-pattern>/facultyloginActionServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudentregActionServlet</servlet-name>
        <url-pattern>/StudentregActionServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudentloginActionServlet</servlet-name>
        <url-pattern>/StudentloginActionServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
